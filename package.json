{
  "name": "@UseringOfficial/css-to-tailwindcss",
  "version": "1.0.0",
  "description": "CSS to TailwindCSS converter",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./lib/",
    "cm": "cz",
    "lint": "eslint src",
    "prepare": "is-ci || husky",
    "semantic-release": "semantic-release",
    "test:watch": "vitest watch",
    "test": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UseringOfficial/css-to-tailwindcss.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=20.0"
  },
  "keywords": [
    "convert",
    "converter",
    "conversion",
    "css",
    "postcss",
    "tailwind",
    "tailwindcss",
    "css-to-tailwind"
  ],
  "bugs": {
    "url": "https://github.com/UseringOfficial/css-to-tailwindcss/issues"
  },
  "homepage": "https://github.com/UseringOfficial/css-to-tailwindcss#readme",
  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/node": "^20.14.8",
    "@types/postcss-safe-parser": "^5.0.1",
    "@vitest/coverage-v8": "3.0.9",
    "commitizen": "^4.3.1",
    "commitlint": "^19.8.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "lint-staged": "^15.5.0",
    "postcss-nested": "^6.0.0",
    "prettier": "^3.5.3",
    "semantic-release": "^24.2.3",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  },
  "dependencies": {
    "colord": "^2.9.3",
    "css-what": "^6.1.0",
    "postcss": "^8.5.3",
    "postcss-safe-parser": "^6.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17"
  },
  "packageManager": "pnpm@9.15.5+sha512.845196026aab1cc3f098a0474b64dfbab2afe7a1b4e91dd86895d8e4aa32a7a6d03049e2d0ad770bbe4de023a7122fb68c1a1d6e0d033c7076085f9d5d4800d4"
